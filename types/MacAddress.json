{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "description": "MacAddress",
  "allOf": [
    {
      "$ref": "file:///types/BasicObject.json#"
    },
    {
      "type": "object",
      "properties": {
        "address": {
          "type": "string",
          "pattern": "^([0-9A-Fa-f]{2}[:-]){5}([0-9A-Fa-f]{2})$"
        }
      },
      "required": [
        "address"
      ]
    }
  ]
}
